    "chromedriver": "^132.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "selenium-webdriver": "^4.18.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
