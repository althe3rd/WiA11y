{
  "name": "accessibility-crawler-backend",
  "version": "1.0.0",
  "description": "Backend for accessibility crawler",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
